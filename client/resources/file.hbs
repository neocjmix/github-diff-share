{{#each this}}
    <article>
        <header>
            <h1>{{from}} -&gt; {{to}}</h1>
        </header>
        <main class="content">
            <ul class="chunks">
                {{#each this.chunks}}  
                    <li>
                        <code>
                            {{#each this.changes}}  
                                <span class="{{type}}">{{content}}</span>
                            {{/each}}
                        </code>
                    </li>
                {{/each}}
            </ul>
        </main>
    </article>
{{/each}}